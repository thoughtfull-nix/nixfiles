[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 100
trim_trailing_whitespace = true
[*.sh]
binary_next_line = true
function_next_line = false
keep_padding = true
shell_variant = auto
space_redirects = true
switch_case_indent = true
